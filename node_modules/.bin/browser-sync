#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/run/media/evgeniy/imDATA/Clouds/Mega/00_Web_дизайн/00_Projects/03_keksby/node_modules/.registry.npmjs.org/browser-sync/2.23.6/node_modules/browser-sync/node_modules:/run/media/evgeniy/imDATA/Clouds/Mega/00_Web_дизайн/00_Projects/03_keksby/node_modules/.registry.npmjs.org/browser-sync/2.23.6/node_modules:/run/media/evgeniy/imDATA/Clouds/Mega/00_Web_дизайн/00_Projects/03_keksby/node_modules/.registry.npmjs.org/browser-sync/node_modules:/run/media/evgeniy/imDATA/Clouds/Mega/00_Web_дизайн/00_Projects/03_keksby/node_modules/.registry.npmjs.org/node_modules:/run/media/evgeniy/imDATA/Clouds/Mega/00_Web_дизайн/00_Projects/03_keksby/node_modules:/run/media/evgeniy/imDATA/Clouds/Mega/00_Web_дизайн/00_Projects/node_modules:/run/media/evgeniy/imDATA/Clouds/Mega/00_Web_дизайн/node_modules:/run/media/evgeniy/imDATA/Clouds/Mega/node_modules:/run/media/evgeniy/imDATA/Clouds/node_modules:/run/media/evgeniy/imDATA/node_modules:/run/media/evgeniy/node_modules:/run/media/node_modules:/run/node_modules:/node_modules:/run/media/evgeniy/imDATA/Clouds/Mega/00_Web_дизайн/00_Projects/03_keksby/node_modules/browser-sync/node_modules" "$basedir/node"  "$basedir/../browser-sync/dist/bin.js" "$@"
  ret=$?
else 
  NODE_PATH="/run/media/evgeniy/imDATA/Clouds/Mega/00_Web_дизайн/00_Projects/03_keksby/node_modules/.registry.npmjs.org/browser-sync/2.23.6/node_modules/browser-sync/node_modules:/run/media/evgeniy/imDATA/Clouds/Mega/00_Web_дизайн/00_Projects/03_keksby/node_modules/.registry.npmjs.org/browser-sync/2.23.6/node_modules:/run/media/evgeniy/imDATA/Clouds/Mega/00_Web_дизайн/00_Projects/03_keksby/node_modules/.registry.npmjs.org/browser-sync/node_modules:/run/media/evgeniy/imDATA/Clouds/Mega/00_Web_дизайн/00_Projects/03_keksby/node_modules/.registry.npmjs.org/node_modules:/run/media/evgeniy/imDATA/Clouds/Mega/00_Web_дизайн/00_Projects/03_keksby/node_modules:/run/media/evgeniy/imDATA/Clouds/Mega/00_Web_дизайн/00_Projects/node_modules:/run/media/evgeniy/imDATA/Clouds/Mega/00_Web_дизайн/node_modules:/run/media/evgeniy/imDATA/Clouds/Mega/node_modules:/run/media/evgeniy/imDATA/Clouds/node_modules:/run/media/evgeniy/imDATA/node_modules:/run/media/evgeniy/node_modules:/run/media/node_modules:/run/node_modules:/node_modules:/run/media/evgeniy/imDATA/Clouds/Mega/00_Web_дизайн/00_Projects/03_keksby/node_modules/browser-sync/node_modules" node  "$basedir/../browser-sync/dist/bin.js" "$@"
  ret=$?
fi
